{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "max-len": ["error", { "code": 120 }],
        "no-console": "off",
        "quotes": ["error", "single"]
    }
}
